<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="slam24.hm.com.holamundo.MainActivity"
    android:orientation="vertical"
    android:id="@+id/sensor">
    <TextView
        android:id="@+id/texto"
        android:text="Login"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"/>

    <EditText
        android:id="@+id/username"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Nombre de Usuario"/>
    <EditText
        android:id="@+id/passwrod"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="ContraseÃ±a"/>
    <Button
        android:id="@+id/toast"
        android:text="Modal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/activitytoactivity"
        android:text="Nueva actividad"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/botonenv"
        android:text="@string/boton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/botonenv2"
        android:text="@string/boton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <ImageView
        android:id="@+id/imagenlogin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/luis"/>

</LinearLayout>
